    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "async-aws/s3": "^1.12",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^4.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/migrations": "^3.2",
        "dragonmantank/cron-expression": "^3.3",
        "guzzlehttp/promises": "^1.4",
        "league/uri": "^6.8",
        "league/uri-components": "^2.4",
        "manyou/bing-homepage": "^0.1.0@dev",
        "manyou/clear-service-tags-bundle": "^0.1.0@dev",
        "manyou/inertia-bundle": "^2.4",
        "manyou/leanstorage": "^0.1.0@dev",
        "manyou/mango": "^0.1.0@dev",
        "manyou/promise-http-client": "^0.1.0@dev",
        "moneyphp/money": "^4.1",
        "monolog/monolog": "^3.0",
        "nyholm/psr7": "^1.5",
        "pentatrion/vite-bundle": "^7.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.11",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/log": "^2.0 || ^3.0",
        "rainbowedge/swoole-runtime": "@dev",
        "symfony/config": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/doctrine-messenger": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/http-client-contracts": "^3.0",
        "symfony/http-client-implementation": "^3.0",
        "symfony/http-foundation": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/lock": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/messenger": "^7.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/scheduler": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/uid": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/yaml": "^7.0",
        "thecodingmachine/safe": "^2.4",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "web-token/jwt-bundle": "*",
        "web-token/jwt-library": "*"
    },
    "require-dev": {
        "ext-intl": "*",
        "blackfire/php-sdk": "^1.31",
        "doctrine/coding-standard": "^10.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^7.0",
        "symfony/cache": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/debug-bundle": "^7.0",
        "symfony/maker-bundle": "^1.47",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/process": "^7.0",
        "symfony/stopwatch": "^7.0",
        "symfony/web-profiler-bundle": "^7.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*",
            "docker": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./swoole-runtime"
        }
    ]
}
