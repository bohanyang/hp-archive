{
    "name": "rainbowedge/swoole-runtime",
    "type": "library",
    "description": "Run Symfony applications with Swoole",
    "keywords": [
        "swoole",
        "symfony",
        "runtime",
        "php-runtime"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brent, Yang Bohan",
            "email": "8384161+bohanyang@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "mezzio/mezzio-swoole": "^4.0",
        "symfony/psr-http-message-bridge": "^2.3|^6.4|^7.0",
        "psr/http-message": "^1.0|^2.0",
        "symfony/runtime": "^6.4|^7.0",
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/http-foundation": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "^1.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Rainbowedge\\SwooleRuntime\\": "src/"
        }
    }
}
